# API Base URL
# This is the base URL for the API that the React application will interact with.
# Typically, this points to your backend server.
REACT_APP_API_BASE_URL=http://localhost:5000/api

# Stripe Public Key (for payment processing)
# This is your public key from Stripe, which is used for processing payments 
# securely on the frontend.
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key

# Frontend Application URL
# This is the URL where your React application is hosted.
# It is used for things like redirecting after a payment or for CORS settings.
REACT_APP_FRONTEND_URL=http://localhost:3000
